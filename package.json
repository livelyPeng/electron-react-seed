{
  "name": "@yp_cloud/electron",
  "version": "0.0.1",
  "author": "pengLei",
  "description": "a electron-react project",
  "main": "./dist/electron/main.js",
  "private": true,
  "scripts": {
    "start:pc": "npm run dev-electron",
    "dev-electron": "node .script/index --dev --electron",
    "dev:web": "node .script/index --dev --web",
    "build:win32": "node .script/index --build --electron --win --ia32",
    "build:win64": "node .script/index --build --electron --win --x64",
    "build:mac": "node .script/index --build --electron --mac",
    "build:dir": "node .script/index --build --electron --dir",
    "build:web": "node .script/index --build --web",
    "lint": "eslint --ext src",
    "lint-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src"
  },
  "build": {
    "extraFiles": [
      "lib"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:25565/"
      }
    ],
    "productName": "yupao",
    "appId": "org.simulatedgreg.electron-react",
    "directories": {
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "assets/icons/icon.ico",
      "uninstallerIcon": "assets/icons/icon.ico",
      "installerHeaderIcon": "assets/icons/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "deleteAppDataOnUninstall": true,
      "artifactName": "${productName}${version}.${ext}",
      "shortcutName": "鱼泡工程云"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "assets/icons/icon.icns"
    },
    "win": {
      "icon": "assets/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": "deb",
      "icon": "assets/icons"
    }
  },
  "dependencies": {
    "axios": "^0.27.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.16.9",
    "@babel/runtime": "^7.16.7",
    "@types/node": "^14.14.41",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "^8.2.5",
    "cfonts": "^2.10.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "del": "^6.1.1",
    "electron": "19.0.0",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-updater": "^5.0.1",
    "esbuild-loader": "^2.19.0",
    "eslint": "7.19.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "multispinner": "^0.2.1",
    "node-sass": "^7.0.1",
    "portfinder": "^1.0.28",
    "sass-loader": "^13.0.0",
    "sass-resources-loader": "^2.2.5",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "thread-loader": "^3.0.4",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  }
}
